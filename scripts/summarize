#!/usr/bin/env python

import sys

run = sys.argv[1]
f = open(run + '.rpt', 'r')
out = open(run + '.summary', 'w+')
line = f.readline()
while not line.startswith('; Fitter Resource Usage Summary'):
        line = f.readline()

line = f.readline()
while line.startswith(';') or line.startswith('+'):
        out.write(line)
        line = f.readline()

f.close()
out.close()
